{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "zhyl-tauri-rust",
  "version": "1.1.0",
  "identifier": "com.zhyl-tauri-rust.app",
  "build": {
    "frontendDist": "../src",
	"devUrl": "https://mass.cqgelin.com/h5"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "zhyl-tauri-rust",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
